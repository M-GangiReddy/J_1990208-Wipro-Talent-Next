import java.util.*;
class PrintTrue 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int l=sc.nextInt();
		int a[]=new int[l];
		int i;
		for (i=0;i<l;i++)
		{
			a[i]=sc.nextInt();
		}
		for(i=0;i<l;i++)
		{
			if(a[i]!=1 && a[i]!=4)
			{
				System.out.println(false);
				break;
			}
			if((i==l-1) && (a[i]==1 ||a[i]==4))
			{
				System.out.println(true);
			}
		}
	}
}
