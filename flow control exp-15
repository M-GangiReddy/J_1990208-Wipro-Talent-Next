import java.util.*;
class Prime 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int x=sc.nextInt();
		int count=0;
		for(int i=2;i<x;i++)
		{
			if(x%i==0)
			{
				count++;
			}
		}
		if(count==0)
		{
			System.out.println("Prime Number");
		}
		else
		{
			System.out.println("Compositive Number");
		}

	}
}
