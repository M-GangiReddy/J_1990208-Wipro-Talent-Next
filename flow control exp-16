import java.util.*;
class Pallindrom
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int temp=n, i, sum=0;
		while(n>0)
		{
			i=n%10;
			n=n/10;
			sum=sum*10+i;
		}
		if(temp==sum)
		{
			System.out.println("pallindrom");
		}
		else
		{
			System.out.println("Not a pallindrom");
		}
	}
}
