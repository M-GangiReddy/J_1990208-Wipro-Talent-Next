class LaunchCopies 
{
	public static void main(String[] args) 
	{
		String s="Wipro";
		StringBuilder s1=new StringBuilder();
		for(int i=0;i<2;i++)
		{
			s1.append(s.charAt(i));
		}
		int l=s.length();
		StringBuilder x=new StringBuilder();
		for(int i=0;i<l;i++)
		{
			x.append(s1);
		}
		System.out.println(x);
	}
}
