import java.util.*;
class SumArray 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the length");
		int l=sc.nextInt();
		int a[]=new int[l];
		int i;
		for(i=0;i<l;i++)
		{
			System.out.println("Enter the "+i+" Number :");
			a[i]=sc.nextInt();
		}
		for(i=0;i<l;i++)
		{
			System.out.println(a[i]);
		}
	}
}
