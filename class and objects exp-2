import java.util.*;
import java.util.*;
class Pow
{
	static int powerInt(int num1, int num2)
	{
		int p=(int)Math.pow(num1, num2);
		return p;
	}
	static double powerDouble(double num1, int num2)
	{
		double p=Math.pow(num1, num2);
		return p;
	}
    
}
class PowNum
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int x=sc.nextInt();
		int y=sc.nextInt();
		System.out.println(Pow.powerInt(x, y));
		System.out.println(Pow.powerDouble(x, y));
        
	}
}
